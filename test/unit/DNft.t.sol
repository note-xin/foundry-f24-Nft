pragma solidity ^0.8.18;

import {Test, console} from "forge-std/Test.sol";
import {DNft} from "src/DNft.sol";
import {Deploy} from "script/Deploy__DNft.s.sol";

/**
 * @dev This tests the functionality of our contract
 * @notice note that the `console.log` statements are for debugging purposes comment them out
 */
contract NftTest is Test{
    /* State Variables */
    uint256 public constant STARTING_BALANCE = 10 ether;

    string public constant NERO_SVG_URI = 
        "data:image/svg+xml;base64,";
    string public constant KURO_SVG_URI = 
        "data:image/svg+xml;base64,";
    string public constant KURO_METADATA_URI =
        "data:application/json;base64,";
    string public constant NERO_METADATA_URI =
        "data:application/json;base64,";
    
    address public immutable i_user = makeAddr("user");

    DNft dnft;
    Deploy deploy;

    /* Functions */
    /* External Functions */
    function setUp() external {
        deploy = new Deploy();
        dnft = deploy.run();
        
        vm.deal(i_user, STARTING_BALANCE);
    }

    /* Test Functions */
    function test_verifyTokenUri() public {
        // Arrange
        vm.prank(i_user);
        dnft.mintNft();

        // Act
        console.log(dnft.tokenURI(0));
    }
    
    function test_verifyOtherCat() public {
        // Arrange
        vm.prank(i_user);
        dnft.mintNft();

        // Act
        vm.prank(i_user);
        dnft.otherCat(0);

        // Assert
        assertEq(
            keccak256(abi.encodePacked(dnft.tokenURI(0))),
            keccak256(abi.encodePacked(NERO_METADATA_URI))
        );
    }
}